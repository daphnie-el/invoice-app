{
  "arrowParens": "always",
  "bracketSpacing": true,
  "endOfLine": "lf",
  "htmlWhitespaceSensitivity": "css",
  "insertPragma": false,
  "bracketSameLine": false,
  "jsxSingleQuote": false,
  "printWidth": 80,
  "proseWrap": "preserve",
  "quoteProps": "as-needed",
  "requirePragma": false,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "es5",
  "useTabs": false,
  "vueIndentScriptAndStyle": false,
   "overrides": [
    {
      "files": "*.ts",
      "options": { "parser": "typescript" }
    },
    {
      "files": "*.tsx",
      "options": { "parser": "typescript" }
    },
    {
      "files": "*.js",
      "options": { "parser": "babel" }
    },
    {
      "files": "*.jsx",
      "options": { "parser": "babel" }
    },
    {
      "files": "*.json",
      "options": { "parser": "json" }
    },
    {
      "files": "*.css",
      "options": { "parser": "css" }
    },
    {
      "files": "*.scss",
      "options": { "parser": "scss" }
    }]
}
